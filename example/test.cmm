func int add(int a, int b) {
    int result =0
    result += a+b;
    return result;  // Adds two integers
}

func float divide(int numerator, int denominator) {
    if (denominator == 0) {
        printf("Error: Division by zero.\n");
        return 0;  // Return 0 for division by zero
    }
    return numerator / denominator;  // Perform division
}

func int main() {
    int x = 10;
    int y = 20;
    float result;

    // Add two numbers
    result = add(x, y);
    printf("Sum: %f\n", result);

    // Divide two numbers
    result = divide(x, y);
    printf("Quotient: %f\n", result);

    // Test division by zero
    result = divide(x, 0);
    
    // Example of a loop
    for (int i = 0; i < 5; i++) {
        printf("Iteration: %d\n", i);
    }

    // Conditional statement
    if (result > 0) {
        printf("Result is positive.\n");
    } else {
        printf("Result is non-positive.\n");
    }

    return 0;
}
